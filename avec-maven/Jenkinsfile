pipeline {
    agent any
    stages {
        stage('Testing Maven') {
                    steps {
                        sh """mvn -version"""
                    }
        }
        stage('Checkout') {
          steps {
            checkout([$class: 'GitSCM', branches: [[name: '*/main']], userRemoteConfigs: [[url: 'https://github.com/Oumayma-Bah/tp_esprit.git']]])
          }
        }
        stage('Afficher la date syst√®me') {
              steps {
                sh 'date'
              }
        }
    }
}
